{% block address_row %}
    <div class="row">
        <div class="col">{{ form_row(form.lineOne) }}</div>
    </div>
    <div class="row">
        <div class="col">{{ form_row(form.lineTwo) }}</div>
        <div class="col">{{ form_row(form.city) }}</div>
    </div>
    <div class="row">
        <div class="col">{{ form_row(form.zipCode) }}</div>
        <div class="col">{{ form_row(form.state) }}</div>
    </div>
    <div class="row">
        <div class="col">{{ form_row(form.country) }}</div>
    </div>
{% endblock address_row %}


{% block _customer_dob_row %}
    <div class="row g-0" id="dob">
        {% for key, child in form.children|reverse %}
            <div class="col{{ key == 'month' ? ' px-1' : '' }}">
                {{ form_row(child, {attr: {'data-choices': '', 'class': 'form-select'}}) }}
            </div>
        {% endfor %}
    </div>
{% endblock _customer_dob_row %}